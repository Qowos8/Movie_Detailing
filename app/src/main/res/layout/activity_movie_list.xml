        app:layout_constraintTop_toTopOf="@+id/tenet_png"
        app:layout_constraintEnd_toStartOf="@+id/rightline"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:contentDescription="@string/todo"
