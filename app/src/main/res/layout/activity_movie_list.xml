<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#191926"
    android:orientation="vertical"
    android:id="@+id/fragment_layout">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/centerline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/leftline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_begin="16dp"
        android:orientation="vertical" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/rightline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:orientation="vertical"
        app:layout_constraintGuide_end="16dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/movies_list"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/little_symb"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="50dp"
        />
    <ImageView
        android:id="@+id/little_symb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/combined_shape"
        app:layout_constraintStart_toEndOf="@id/leftline"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="52dp"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bg"
        android:src="@drawable/bg"
        app:layout_constraintTop_toBottomOf="@id/little_symb"
        app:layout_constraintStart_toEndOf="@id/leftline"
        android:layout_marginTop="40dp"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/picture"
        android:src="@drawable/movie"
        app:layout_constraintTop_toTopOf="@id/bg"
        app:layout_constraintStart_toStartOf="@id/mask_avenger"
        app:layout_constraintEnd_toEndOf="@id/mask_avenger"
        app:layout_constraintBottom_toBottomOf="@+id/mask_avenger"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:id="@+id/mask_avenger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/mask"
        app:layout_constraintStart_toStartOf="@id/bg"
        app:layout_constraintTop_toTopOf="@id/bg"
        app:layout_constraintEnd_toEndOf="@id/bg"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/like"
        android:src="@drawable/like"
        app:layout_constraintTop_toTopOf="@id/picture"
        app:layout_constraintStart_toEndOf="@id/leftline"
        android:layout_marginStart="145dp"
        android:layout_marginTop="5dp"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:id="@+id/rectangle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle"
        app:layout_constraintStart_toStartOf="@id/picture"
        app:layout_constraintTop_toTopOf="@id/picture"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:contentDescription="@string/todo"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/age_rate"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="@id/rectangle"
        app:layout_constraintTop_toTopOf="@id/rectangle"
        android:textStyle="bold" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horiz"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="354dp"
        app:layout_constraintTop_toBottomOf="@id/picture" />
    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/avengers_end_game"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/horiz"
        app:layout_constraintStart_toStartOf="@id/janr"
        android:layout_marginTop="10dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/avengers_time"
        android:textColor="@color/cardview_dark_background"
        app:layout_constraintTop_toBottomOf="@+id/name"
        app:layout_constraintStart_toStartOf="@+id/name"
        android:textSize="11sp" />
    <TextView
        android:id="@+id/janr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/janr"
        android:textColor="#FF3466"
        android:textSize="11sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/bg"
        app:layout_constraintBottom_toTopOf="@+id/horiz"
        android:layout_marginBottom="19dp"
        android:layout_marginStart="8dp"/>
    <ImageView
        android:id="@+id/First"
