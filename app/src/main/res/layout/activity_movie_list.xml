        android:layout_height="wrap_content"
        android:src="@drawable/starbr"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/janr_wonder_woman"
        app:layout_constraintStart_toEndOf="@+id/fourth_star_wonder"
        android:contentDescription="@string/todo"/>
    <TextView
        android:id="@+id/review_wonder_woman"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/review_wonder_woman"
        android:textColor="#6D6D80"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/fifth_star_wonder"
        app:layout_constraintTop_toBottomOf="@+id/janr_wonder_woman" />
    <TextView
        android:id="@+id/name_wonder_woman"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/wonder_woman_name"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/janr_wonder_woman"
        app:layout_constraintStart_toStartOf="@+id/janr_wonder_woman"
        android:layout_marginTop="20dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/wonder_woman_time"
        android:textColor="@color/cardview_dark_background"
        app:layout_constraintStart_toStartOf="@+id/janr_wonder_woman"
        app:layout_constraintTop_toBottomOf="@+id/name_wonder_woman"
        android:textSize="11sp" />

</androidx.constraintlayout.widget.ConstraintLayout>